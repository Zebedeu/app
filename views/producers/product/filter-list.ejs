<table class="table shopping-cart-wrap">
	<tbody>
		<tr>
		  <th scope="col" style="width:350px !important;"><%= labels['LBL_PRODUCT'][language] %></th>
		  <th scope="col" width="60"><%= labels['LBL_UNIT'][language] %></th>
		  <th scope="col" width="150"><%= labels['LBL_VARIETY'][language] %></th>
		  <th scope="col" width="100"><%= labels['LBL_SIZE'][language] %> </th>
		  <th scope="col" width="120"><%= labels['LBL_QUANTITY'][language] %></th>
		  <th scope="col" width="120"><%= labels['LBL_HARVEST_DATE'][language] %></th>
		  <th scope="col" width="120"><%= labels['LBL_STATUS'][language] %></th>
		  <th scope="col" width="100"><%= labels['LBL_ACTION'][language] %></th>
		</tr>
		<% for(let i=0; i < products.length; i++) { %>
			<tr>
				<td>
					<figure class="">
						<div class="img-wrap"><img src="<%= products[i].images[0] %>" class="img-thumbnail img-sm"></div>
						<figcaption class="media-body kp_myorder">
							<h3><%= products[i].sub_category_title %></h3>
							<h4><%= products[i].category_title %> &nbsp; <i style="font-size: 14px;" class="fa fa-map-marker" aria-hidden="true"></i> <span><%= products[i].location %></span></h4>
							<h4 style="color: #f69624;" class="sku_wrapper">Kz <%= (products[i].unit_price) %> / <%= products[i].unit_type %></h4>
						</figcaption>
					</figure> 
				</td>
				<td> 
					<h4 class='capitalize'><%= products[i].unit_type %></h4>	
				</td>
				<td> 
					<h4><%= products[i].product_variety_title %></h4>	
				</td>
				<td> 
					<h4><%= products[i].size %></h4>	
				</td>
				<td> 
					<h4 class='capitalize'><%= products[i].remaining_unit_value %> <%= products[i].unit_type %></h4>	
				</td>
				<td> 
					<h4><%= products[i].harvest_date %></h4>	
				</td>
				<td> 
					<h4><%= products[i].status %></h4>	
				</td>
				<td> 
					<div style="display: inline-block;background-color: #f69624;padding: 10px;width: 39px;height: 39px;" class="">
						<a href="<%=baseURL%>producers/product/display/<%= products[i].product_id %>"><i style="font-size: 22px;color: #fff;" class="fa fa-edit"></i></a>
					</div>
					<div style="display: inline-block;background-color: #aa1a2b;padding: 10px;width: 39px;height: 39px;"  class="">
						<a href="<%=baseURL%>producers/product/remove/<%= products[i].product_id %>"><i style="font-size: 22px;color: #fff;" class="fa fa-trash"></i></a>
					</div>
				</td>									
			</tr>
		<% } %>
	</tbody>
</table>