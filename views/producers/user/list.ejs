<section class="productsdetail">
	<div class="container">
		<div class="breadcrumb_card">
			<%- include('../../shared/breadcrumb') %>
		</div>
		<div class="product_card">
			<div class="row add-address post-add">
				<div class="col-md-12 col-sm-12 col-xs-12">
			 		<h3><%= labels['LBL_USERS_LIST'][language] %></h3>
				</div>
				<% for(let i=0; i < farmers.length; i++) { %>
					<div class="col-lg-12 col-sm-12 my_oderDetails">
						<div class="col-lg-3 col-md-6 col-sm-12 kp_myorder">
							<img src="<%= farmers[i]['photo'] %>">
						</div>
						<div class="col-lg-8 col-md-6 col-sm-12 kp_myorder">
							<h2><%= farmers[i]['name'] %></h3>
							<h3><i class="fa fa-envelope-o" aria-hidden="true"></i> <%= farmers[i]['email'] %></h3>
							<h3><i class="fa fa-phone" aria-hidden="true"></i> <%= farmers[i]['phone_number'] %></h3>
							<div class="kp_location">
								<i class="fa fa-map-marker" aria-hidden="true"></i> <span><%= farmers[i]['address'] %></span>
							</div>
						</div>
						<div class="edit_ico">
							<a href="<%=baseURL%>producers/user/display/<%= farmers[i].farmer_id %>"><i class="fa fa-edit"></i></a>
						</div>
						<div class="delete_ico">
							<a href="<%=baseURL%>producers/user/remove/<%= farmers[i].farmer_id %>"><i class="fa fa-trash"></i></a>
						</div>
					</div>
				<% } %>
			</div>
		</div>
	</div>
</section>
<section class="productsdetail">
	<div class="container">
		<div class="col-md-12 thankyou" style="margin: -20px 0 20px 0;">
			<a href="add"><button class="main_btn"><%= labels['LBL_ADD_USER_NOW'][language] %></button></a>
		</div>
	</div>
</section>